{
  "taskDefinition": {
    "family": "identity",
    "containerDefinitions": [
      {
        "name": "identity",
        "image": "544757169931.dkr.ecr.eu-west-2.amazonaws.com/identity:latest",
        "cpu": 0,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "essential": true,
        "healthCheck": {
          "retries": 10,
          "command": [
            "CMD-SHELL",
            "curl --silent --fail localhost/probe || exit 1"
          ],
          "timeout": 10,
          "interval": 60,
          "startPeriod": 60
        }
      }
    ]
  }
}
